<%= form_for band  do |f| %>
	<%= f.label :name, "Name:" %>
	<%= f.text_field :name, placeholder: "Name", class: "form-control" %>
	<%= f.submit nil, class:"btn btn-primary", id:"submit" %>
<% end %>

